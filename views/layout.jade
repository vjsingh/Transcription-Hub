    <div id="fb-root"></div>
    #entire-page
      #entire-page-minus-footer
        .navbar
          .navbar-inner
            .container-fluid
              a.brand(href='/')
                img#logoTopbar(src='/images/saxIcon.png', width=30, height=30)
                .text
                  | Transcription Hub
              ul.nav
                li
                  a(href='/about') About
                - if (userLoggedIn)
                  li
                    a(href='/Upload') Upload
                - else
                  li
                    a(href='/register') Register
                li
                  a(href='/bounty') Requests
                li
                  a(href='/browse') Browse
              //a.button.btn.primary.pull-left(href="/upload") Upload
              //a.button.btn.primary.pull-left(href="/register") Register
              form.navbar-search.pull-left(method='get', action='/search')
                input#searchType.jqueryHide(type='text', name='search[type]', value='transcriptions')
                input.search-query(type='text', name='search[omniSearch]', placeholder='Enter Artist/Song/Album/Instrument')
                input(type='submit', style='position: absolute; left: -9999px;') // hide submit button
              .pull-right.navbar-text
                - if (userLoggedIn)
                  .right
                    | Logged in as:
                    a#profile(href='/profile')
                      = user.username
                    a#logout(href='/logout') Log Out
                - else
                  script(type='text/javascript', src='/javascripts/login.js')
                  form.navbar-form#loginTopBar(method='post', action'=/login')
                    != partial('loginFields', {locals: {user: user}})
        != body
      #footer
        .inner.container-fluid
          .row-fluid
